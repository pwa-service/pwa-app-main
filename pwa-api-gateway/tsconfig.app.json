{
  "extends": "./tsconfig.app.json",
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "ES2020"
    ],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "useDefineForClassFields": false,
    "allowSyntheticDefaultImports": true,
    "strictPropertyInitialization": false,
    "baseUrl": "../",
    "paths": {
      "@pwa/prisma": [
        "./pwa-prisma/src"
      ],
      "@pwa/prisma/*": [
        "./pwa-prisma/src/*"
      ]
    }
  }
}